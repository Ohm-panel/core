<h1>Editing domain</h1>

<% form_for(@domain) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= @domain.domain %>
  </p>
  <p>
    <%= f.label :path %><br />
    <%= f.text_field :path %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Back', :controller => 'dashboard' %>

